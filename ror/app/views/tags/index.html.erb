<p style="color: green"><%= notice %></p>

<h1>Tags</h1>

<div id="tags">
  <table>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
    <% @tags.each do |tag| %>
        <tr>
          <th><%= tag.name %></th>
          <th>
            <%= link_to "Edit", edit_tag_path(tag) %>
          </th>
        </tr>
    <% end %>
  </table>
</div>

<%= link_to "New tag", new_tag_path %>
